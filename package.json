{
  "name": "@ligalaiz/cart",
  "version": "0.0.1",
  "description": "Shopping cart",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack --config config/webpack.prod.js --profile --json=compilation-stats.json",
    "start:client": "npm run clean && webpack serve --config config/webpack.dev.js",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf dist",
    "test": "jest --forceExit",
    "test:smoke": "jest --forceExit --t=#smoke",
    "test:test": "jest --forceExit --t=#test",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ligalaiz/cart.git"
  },
  "keywords": [
    "cart",
    "shopping cart"
  ],
  "author": "Ligalaiz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ligalaiz/cart/issues"
  },
  "homepage": "https://github.com/Ligalaiz/cart#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.6",
    "@squoosh/lib": "^0.4.0",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "*",
    "compile-ejs-loader": "^0.0.3",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cssnano": "^5.1.12",
    "dotenv-webpack": "^7.1.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^3.1.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-localstorage-mock": "^2.4.21",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "npm": "^8.13.2",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "sugarss": "^4.0.1",
    "svg-sprite-loader": "^6.0.11",
    "svgo": "^2.8.0",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm test:smoke"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint:fix",
      "npm run test:smoke"
    ]
  }
}
